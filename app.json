{
  "expo": {
    "name": "Datadget",
    "slug": "datadget",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "appleTeamId": "85344DK5P3",
      "bundleIdentifier": "com.datadget",
      "entitlements": {
        "com.apple.security.application-groups": [
          "group.com.datadget"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.datadget"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#3787e2"
        }
      ],
      "expo-localization",
      "expo-secure-store",
      "@bugsnag/plugin-expo-eas-sourcemaps",
      "expo-font",
      "expo-web-browser",
      "@bacons/apple-targets"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "bugsnag": {
        "apiKey": "d875c79e1469523af898077a3e5c3099"
      }
    }
  }
}
